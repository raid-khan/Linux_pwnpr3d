HardwareComputer computer = new HardwareComputer()
OperatingSystem os = computer.newOperatingSystem()
Host = newHost.ip()
	/ = new Directory()
	Host.connect(/)
	usr = new Directory()
		applications = new Directory
			gnome-calculator = new ApplicationService
			applications.connect(gnome-calculator)
			gnome-desktop3 = new ApplicationService
			applications.connect(gnome-desktop3)
		usr.connect(applicaitons)
		rhosts = new Directory()
		bin = new Directory()
		sbin = new Directory()
		lib = new Directory()
		share = new Directory()
			doc = new Dataobject()
			share.connect(doc)
		include = new Directory()
		local = new Directory()
		games = new Directory()
		usr.connect(rhosts)
		usr.connect(bin)
		usr.connect(sbin)
		usr.connect(lib)
		usr.connect(share)
		usr.connect(include)
		usr.connect(local)
		usr.connect(games)
	Host.connect(usr)
	etc = new Directory()
		netconfig = new Directory()
			ARP = new InfrastructureService()
			Sendmail = new InfrastructureService()
			TCP/UDP = new InfrastructureService()
			Telnet = new InfrastructureService()
			FTP = new InfrastructureService()
			etc.connect(netconfig)
			etc.connect(ARPtable)
			etc.connect(Sendmail)
			etc.connect(TCP/UDP)
			etc.connect(Telnet)
			etc.connect(FTP)
		etc.connect(netconfig)
		Networks = new Directory()
			interfaces = new ApplicationInfrasructure()
			Networks.connect(interfaces)
		etc.connect(Networks)
		passwd = new Dataobject()
				ADDusr = new InfrastructureFunction()
				passwd.connect(ADDusr)
					Alice(previlige) = new Role()
					Bob(restricted) = new Role()
					ADDusr.connect(Alice,trusted)
					ADDusr.connect(Bob,trusted)
				AccessLists = new ApplicationFunction()
					Alice.connect(AccessLists)
					Bob.connect(AccessLists)
				passwd.connect(Acceslists)
		etc.connect(passwd)
		calendar = new ApplicationService()
		etc.connect(calendar)
		rc.d = new InfrastructureFunction()
		etc.connect(rc.d)
		fonts = newFile()
		etc.connect(fonts)
		init.d = new InfrastructureFunction()
		etc.connect(init.d)
		crontab = new InfrastructureFunction()
		etc.connect(crontab)
		idap = new InfrastructureService()
		etc.connect(ldap)
	Host.connect(etc)
	bin = new Directory()
		grep = new ApplicationFunction()
		mount = new InfrastructureFunction()
		kill = new ApplicationFunction()
		netstat = new ApplicationFunction()
		bin.connect(grep)
		bin.connect(mount)
		bin.connect(kill)
		bin.connect(netstat)
	Host.connect(bin)
	sbin = new Directory
	Host.connect(sbin)
	var = new Directory()
		run = new ApplicationFunction()
		logs = new Directory()
		tmp = new Directory()
		lib = new Directory()
		www = newHost,ip()
			html = new ApplicationService
			config = new Directory()
			test = new Directory()
			gnome-desktop3Style = new ApplicationFunctoin()
			index = new ApplicationFunction()
			www.connect(html)
			www.connect(config)
			www.connect(gnome-desktop3style)
			www.connect(test)
			www.connect(index)
				HostFirewall = new HostFirewall()
					HostFirewall.permit(rhosts.ip , port.80)
					HostFirewall.permit(rhosts.ip , port.22)
					HostFirewall.permit(rhosts.ip , port.23)
					HostFirewall.permit(rhosts.ip , port.53)
					NetworkAccessControl = newApplicationService
					HostFirewall.connect(NetworkAccessControl)
				www.connect(HostFirewall)
				router = newrouter()
				HostFirewall.connect(router,trusted)
			/.connect(www)
		cache = new Directory()
		var.connect(run)
		var.connect(logs)
		var.connect(tmp)
		var.connect(lib)
		var.connect(www)
		var.connect(cache)
	Host.connect(var)
	mnt = new Directory()
		cd-rom = new Infrastructureinterface()
		floppy = new Infrastructureinterface()
		mnt.connect(cd-rom)
		mnt.connect(floppy)
	Host.connect(mnt)
